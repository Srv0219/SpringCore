<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
      xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
      xmlns:context = "http://www.springframework.org/schema/context"
      xsi:schemaLocation = "http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">
      <context:annotation-config/>
      <!-- bean definitions go here -->
      <bean class="com.springcore.autowire.annotation.Address" name="address1">
<property name="street" value="kothruth road"/>
<property name="city" value="Pune"/>
</bean>

<bean class="com.springcore.autowire.annotation.Address" name="address2">
<property name="street" value="Panposh road"/>
<property name="city" value="Rourkela"/>
</bean>

<bean class="com.springcore.autowire.annotation.Employee" name="employee"/>
      
</beans>